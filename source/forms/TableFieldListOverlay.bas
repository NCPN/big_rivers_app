Private Sub Form_Open(Cancel As Integer)
On Error GoTo Err_Handler
            
    Dim i As Integer
    Dim strControl As String
    
    'hide dropdowns (1-60)
    For i = 1 To 60
        strControl = "lblColumnName" & i
        
        Me.Controls(strControl).Visible = False
    
    Next

Exit_Handler:
    Exit Sub
Err_Handler:
    Select Case Err.Number
      Case Else
        MsgBox "Error #" & Err.Number & ": " & Err.Description, vbCritical, _
            "Error encountered (#" & Err.Number & " - Form_Open[TableFieldListOverlay form])"
    End Select
    Resume Exit_Handler
End Sub

