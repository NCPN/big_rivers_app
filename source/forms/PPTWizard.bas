' ---------------------------------
' Sub:          tabSteps_Change
' Description:  Tab click actions
' Assumptions:  -
' Parameters:   -
' Returns:      -
' Throws:       none
' References:   -
' Source/date:  Bonnie Campbell, January 2, 2018 - for NCPN tools
' Adapted:      -
' Revisions:
'   BLC - 1/2/2018 - initial version
' ---------------------------------
Private Sub tabSteps_Change()
On Error GoTo Err_Handler
    
    Dim step As Integer
    
    'defaults
    btnPrev.Enabled = True
    btnNext.Enabled = True
    btnFinish.Enabled = False
    
    Select Case tabSteps.Value
        Case Is = 0 'step 1
            btnPrev.Enabled = False
            lblDirInfo.Caption = "Choose the info to display w/ the photo(s)"
        Case Is = 1 'step 2
            lblDirInfo.Caption = "Choose the slide orientation for the photo(s)"
        Case Is = 2 'step 3
            lblDirInfo.Caption = "Choose if the slides should have a page title or major section title"
        Case Is = 3 'step 4
            btnNext.Enabled = False
            btnFinish.Enabled = True
            lblDirInfo.Caption = "Click Export to create the slides"
        Case Else
            
    End Select

    step = tabSteps.Value + 1
    Me.Title = "PPT Wizard - Step " & step & " of " & tabSteps.Pages.Count
    
Exit_Handler:
    Exit Sub

Err_Handler:
    Select Case Err.Number
      Case Else
        MsgBox "Error #" & Err.Number & ": " & Err.Description, vbCritical, _
            "Error encountered (#" & Err.Number & " - tabSteps_Change[PPTWizard form])"
    End Select
    Resume Exit_Handler
End Sub

