dbMemo "SQL" ="PARAMETERS pkcode Text ( 4 ), scode Text ( 2 );\015\012SELECT f.ID, f.Feature\015"
    "\012FROM ((Feature AS f LEFT JOIN Site_Feature AS sf ON sf.Feature_ID = f.ID) LE"
    "FT JOIN Site AS s ON s.ID = sf.Site_ID) LEFT JOIN Park AS p ON p.ID = s.Park_ID\015"
    "\012WHERE p.ParkCode = [pkcode] \015\012AND s.SiteCode = [scode] \015\012AND s.I"
    "sActiveForProtocol = 1\015\012ORDER BY f.Feature;\015\012"
